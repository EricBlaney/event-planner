<br>
<%= form_with model: Event.new do |form| %>
<%= form.label :title %>
<%= form.text_field :title, placeholder: "Event name" %>  <br>
<%= form.label :description %>
<%= form.text_field :description, placeholder: "Write about your event here!" %>  <br>
<%= form.label :location %>
<%= form.text_field :location, placeholder: "Where are we going?!" %>  <br>
<%= form.label :date %>
<%= form.date_field :date %>  <br>
<%= form.submit "Submit"%>
<% end %>